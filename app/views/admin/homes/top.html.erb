<div class="container">
  <div class="row mt-3">
    <div class="col-lg-3">
      <p class="text-left text-success"><%= notice %></p>
    </div>
    <div class="col-lg-6">
    <% @end_users.each do |end_user| %>
      <div class="row mt-3">
        <ul class="list-unstyled">
          <li>
            <%= link_to admin_end_user_path(end_user.id), class: "text-dark" do %>
              <%= image_tag end_user.get_profile_image, style: "border-radius: 50%; width: 70px; height: 70px; object-fit:cover;" %>
              <%= end_user.name %><br>
            <% end %>
          </li>
        </ul>
      </div>
    <% end %>
    </div>
  </div>
</div>
