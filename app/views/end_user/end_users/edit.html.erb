<h2>プロフィール編集</h2>

<%= form_with model: @end_user, url: end_user_path do |f| %>
  <label for="profile_image">プロフィール画像</label>
  <%= f.file_field :profile_image, id:"profile_image", accept: "image/*" %><br>
  <label for="end_user_name">ユーザ名</label>
  <%= f.text_field :name, id:"end_user_name" %><br>
  <label for="end_user_introduction">自己紹介</label>
  <%= f.text_field :introduction, id:"end_user_introduction" %><br>
  <%= f.submit "保存" %>
<% end %>